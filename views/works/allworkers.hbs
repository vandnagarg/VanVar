{{#each data}}
<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
	<div class="thumbnail" style="width:300px">
		<img src="../images/user.png" alt="" >
					<div class="caption">
						<h3>{{this.name}}</h3>
						<p>
							<label for="phnNumber">Phone No. </label> :{{this.phnNumber}}<br>
							<label for="Age">Age. </label> :{{this.age}}<br>
							<label for="city">City </label> :{{this.city}}<br>
							<label for="work">Work </label> :{{this.work}}<br>

						</p>
						<p class="clearfix">
							<span class="btn-primary "><a href="../../profile/{{this.phnNumber}}"  style="color:white">View More</a></span>
						<span class="btn-primary"><a href="../../create/review/{{this.phnNumber}}" style="color:white">Add Review</a>
							</span>

						</p>
					</div>
	</div>
</div>
{{/each}}